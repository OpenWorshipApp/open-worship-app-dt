{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://wwww.openworship.app/schemas/bibleNewLinesSchema.json",
    "type": "array",
    "description": "List of newline characters used in the Bible translation",
    "items": {
        "type": "string",
        "pattern": "^[123A-Z]{3} [1-9][0-9]{0,2}:[1-9][0-9]{0,2}$",
        "examples": ["MAT 7:6", "MAT 7:13"]
    },
    "uniqueItems": true
}
