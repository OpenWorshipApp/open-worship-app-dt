{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "electron/**/*.ts"
  ]
}
